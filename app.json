      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#3e3e3e"
      },
      "package": "com.campotv.sgaatendimento",
      "versionCode": 8,
      "permissions": ["VIBRATE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
