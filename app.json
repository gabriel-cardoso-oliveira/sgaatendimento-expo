{
  "expo": {
    "name": "SGA Atendimento",
    "slug": "sgaatendimento",
    "version": "1.3.3",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3e3e3e"
    },
